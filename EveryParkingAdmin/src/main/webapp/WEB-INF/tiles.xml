<tiles-definitions>
	<!-- definition name에 test/*/*을 하면 test/{1}/{2} 이렇게 작성해주면 됨 -->
	
  <definition name="parkingManage/*" template="/WEB-INF/views/inc/parkingCommonManageLayout.jsp">
    <put-attribute name="title" value="우주 관리자" />
